<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--工具栏是否居中标题-->
    <attr name="toolbarCenterTitle" format="boolean" />

    <!--框架中的工具栏-->
    <declare-styleable name="BoxToolBar">
        <!-- 是否是居中标题 -->
        <attr name="isCenterTitle" format="boolean" />
        <attr name="loadActivityLabel" format="boolean" />
        <attr name="defaultNavigation" format="boolean" />
    </declare-styleable>

    <!--自动文字大小调整-->
    <declare-styleable name="AutofitTextView">
        <!--最小文字大小-->
        <attr name="autofit_minTextSize" format="dimension" />
        <!--精度，精度越低耗时越高-->
        <attr name="autofit_precision" format="float" />
        <!--是否自动调整-->
        <attr name="autofit_sizeToFit" format="boolean" />
    </declare-styleable>

    <!--正方形布局-->
    <declare-styleable name="SquareLayout">
        <attr name="basisAxis">
            <enum name="anto" value="0" />
            <enum name="basisWidth" value="1" />
            <enum name="basisHeight" value="2" />
        </attr>
    </declare-styleable>

    <!--蒙版输入框-->
    <declare-styleable name="MaskEditText">
        <!--占位格式 #为占位符-->
        <attr name="met_mask" format="string" />
    </declare-styleable>

    <!--带动画CheckBox-->
    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>

</resources>